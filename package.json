{
  "name": "ci-cd",
  "private": true,
  "version": "0.0.9",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --max-warnings=0",
    "test": "vitest run",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "eslint": "^9.28.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "vite": "^5.0.8",
    "vitest": "^3.2.1"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "release-it": {
    "git": {
      "commitMessage": "v${version}",
      "tagName": "v${version}",
      "tagAnnotation": "v${version}",
      "push": true
    },
    "github": {
      "release": true,
      "releaseName": "v${version}",
      "releaseNotes": "v${version}"
    }
  }
